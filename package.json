{
  "name": "whatyoueat",
  "version": "1.0.0",
  "description": "Aplicación de seguimiento nutricional para pacientes y médicos",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "node --no-deprecation server.js",
    "dev": "nodemon --no-deprecation server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --config ./config/jest/jest.config.js",
    "test:unit:mock": "USE_MOCKS=true jest --config ./config/jest/jest.config.js",
    "test:integration": "jest --config ./config/jest/jest.integration.config.js",
    "test:api": "jest --config ./config/jest/jest.api.config.js",
    "test:e2e": "jest --config ./config/jest/jest.e2e.config.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:e2e",
    "test:all:mock": "npm run test:unit:mock && npm run test:integration && npm run test:api && npm run test:e2e",
    "test:coverage": "jest --coverage",
    "test:cleanup": "node ./tests/cleanup-database.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.49.4",
    "bcrypt": "^5.1.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-fileupload": "^1.5.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "sharp": "0.34.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "puppeteer": "24.6.1",
    "supertest": "^7.1.0"
  }
}
